{
  "alias": [
    "chores.piet.me",
    "scripts.piet.me",
    "img.piet.me",
    "www.pietvanzoen.com",
    "pietvanzoen.com"
  ],
  "env": {
    "PINBOARD_TOKEN": "@pinboard-token"
  },
  "builds": [{ "src": "**/index.js", "use": "@now/node" }],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
